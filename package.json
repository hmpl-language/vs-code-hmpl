{
  "name": "hmpl",
  "displayName": "HMPL",
  "description": "Official HMPL language support for VS Code",
  "version": "0.0.3",
  "author": "HMPL.js",
  "publisher": "hmpljs",
  "engines": {
    "vscode": "^1.95.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hmpl-language/vs-code-hmpl"
  },
  "categories": [
    "Programming Languages"
  ],
  "icon": "images/logo.png",
  "contributes": {
    "languages": [
      {
        "id": "hmpl",
        "aliases": [
          "HMPL",
          "hmpl"
        ],
        "extensions": [
          ".hmpl"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "hmpl",
        "scopeName": "text.html.basic",
        "path": "./syntaxes/html.tmLanguage.json"
      }
    ],
    "iconThemes": [
      {
        "id": "hmpl-icons",
        "label": "HMPL File Icons",
        "path": "./file-icons.json"
      }
    ]
  }
}
